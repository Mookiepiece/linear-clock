{
  "name": "linear-clock",
  "version": "0.1.0",
  "license": "WTF2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "serve": "vite preview",
    "check": "tsc --incremental --noEmit",
    "gh": "node ./gh.mjs",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,html,yml,css,scss,md,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^10.10.1",
    "dayjs": "^1.11.11",
    "vue": "3"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^9.4.0",
    "eslint-plugin-vue": "^9.26.0",
    "gh-pages": "^6.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
